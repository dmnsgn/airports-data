    "node-fetch": "^2.2.0",
    "number-is-float": "^1.0.0"
  },
  "keywords": [
    "data",
    "airports",
    "static",
    "dynamic",
    "dump",
    "json",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dmnsgn/airports-data.git"
  },
  "homepage": "https://github.com/dmnsgn/airports-data",
